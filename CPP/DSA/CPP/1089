// 1089. Duplicate Zeros using a extract


#include <iostream>
#include <vector>
using namespace std;


int main(){
    vector<int> arr = {1,0,2,3,0,4,5,0};
    int n = arr.size();
    
    vector<int> result;
    
    for(int i = 0; i < n; i++){
        if(arr[i] == 0){
            result.push_back(0);
            result.push_back(0);
        } else {
            result.push_back(arr[i]);
        }        
        if(result.size() >= n){
            break;
        }
    }
    
    for(int i = 0; i < n; i++){
        arr[i] = result[i];
    }
    
    cout << "modifi: ";
    for(int i = 0; i < n; i++){
        cout << arr[i] << " ";
    }
    cout << endl;
    
    return 0;
}